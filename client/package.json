{
  "name": "nodedice_client",
  "version": "2.0.4",
    "keywords": [
    "open source",
    "node.js",
    "nodejs dice bitcoin btc faucet"
  ],
  "homepage": "https://github.com/popmanhe/node_dice",
  "author": "Neo He",
  "repository": {
    "type": "git",
   "url": "https://github.com/popmanhe/node_dice"
  },
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "prod": "npm run prebuild && babel-node tools/build.js",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "license": "MIT",
  "dependencies": {
    "cookieparser": "0.1.0",
    "moment": "2.18.1",
    "object-assign": "4.1.1",
    "immutability-helper": "2.2.0",
    "ramda": "0.23.0",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-addons-transition-group": "15.4.2",
    "react-bootstrap": "0.30.8",
    "react-cookie": "1.0.5",
    "react-dom": "16.0.1",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-syntax-highlighter": "5.1.2",
    "redux": "3.6.0",
    "redux-saga": "0.14.3",
    "socket.io-client": "1.7.3",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-jest": "19.0.0",
    "babel-loader": "7.0.0",
